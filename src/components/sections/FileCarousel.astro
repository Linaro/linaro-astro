---
import { getEntry } from "astro:content";
import Carousel from "./Carousel.astro";
import type { fileCarouselSchema } from "@/content/schemas/sections";
import type { z } from "astro/zod";

type Props = z.infer<typeof fileCarouselSchema>;
const { filename } = Astro.props;
const { data: images } = await getEntry("data", filename.id);
---

<Carousel images={images} />
